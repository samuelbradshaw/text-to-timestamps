    "time_offset": 0,
    "csv_delimiter": ",",
    "json_indent": 2,
    "json_as_js": false,
    "webvtt_timestamp_tags": false,
    "include_text": true,
    "include_prefixes": true,
    "include_suffixes": true,
    "include_start_times": true,
    "include_end_times": true,
    "include_start_offsets": false,
    "include_end_offsets": false,
    "include_block_numbers": false,
    "include_phrase_numbers": false,
    "include_word_numbers": false
  },
  "additional_metadata": {}
}
