{
  "general": {
    "workfiles_directory": "text_to_timestamps_workfiles",
    "output_directory": "text_to_timestamps_output",
    "overwrite_previous_workfiles": false,
    "overwrite_previous_output": true,
    "voice_isolation_method": null
  },
  "transcribe": {
    "method": "stable-ts-mlx-whisper",
    "model_size": "small",
    "model_name": null,
    "batch_size": 16
  },
  "align": {
    "method": "stable-ts-mlx-whisper-align",
    "model_size": "small",
    "model_name": null,
    "batch_size": 16
  },
  "output": {
    "format": ["csv"],
    "granularity": ["word"],
    "break_phrases_at": ["single_line_breaks", "character_count"],
